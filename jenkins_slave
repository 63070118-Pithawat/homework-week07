pipeline {
agent any
parameters { 
    string(name: 'NAME', defaultValue: '', description:'user Name') 
    string(name: 'Ages', defaultValue: '', description:'user Ages')
}
stages {
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('run python script') {
            steps {
                echo "python myscript.py ${params.NAME} --age ${params.Ages}"
            }
        }
    }

}
